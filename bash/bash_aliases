alias evalssh='eval "$(ssh-agent -s)" && ssh-add ~/.ssh/id_rsa'
alias ll='ls -lh'
alias la='ls -lah'
alias vi='vim'
alias killfirefox="kill $(ps aux | grep firefox$ | awk '{print $2}')"
alias pacmanautoremove='pacman -Qtdq | sudo pacman -Rns -'
alias pacmanexplicitinstalled='pacman -Qqem'
alias gs='git status'
alias gc='git commit'
alias gb='git checkout'
alias flameshot='flameshot gui'
alias td='vi ~/Documents/todo.md'
alias mountnas='sudo mount -t cifs -o vers=2.0,username=rdiep,domain=osmozis //nas.osmoziswifi.com/osmozis /mnt/nas/'
alias mounthome='sudo mount -t cifs -o vers=2.0,username=rdiep,domain=osmozis //nas.osmoziswifi.com/home /mnt/home/'
